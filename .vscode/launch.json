        },
        {
            "name": "Make Cube",
            "type": "python",
            "request": "launch",
            "module": "data.makecube",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Check Cube",
            "type": "python",
            "request": "launch",
            "module": "data.checkcube",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Check Cube - Rectangle",
            "type": "python",
            "request": "launch",
            "module": "data.checkcube_rect",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Train",
            "type": "python",
            "request": "launch",
            "module": "model.train",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}